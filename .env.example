# Site URL (for RSS, sitemap, etc.)
PUBLIC_SITE_URL=https://yoursite.com

# Cal.com booking link (for office hours)
PUBLIC_CALENDAR_URL=https://cal.com/yourname

# Webmentions (setup later)
WEBMENTION_IO_TOKEN=your_token_here

# Analytics (optional)
PUBLIC_PLAUSIBLE_DOMAIN=yoursite.com

# GitHub OAuth for CMS (get from https://github.com/settings/developers)
GITHUB_CLIENT_ID=your_github_client_id_here
GITHUB_CLIENT_SECRET=your_github_client_secret_here
